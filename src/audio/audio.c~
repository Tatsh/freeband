#include "../freeband.h"
#include "audio.h"

bool audio_verifyFiletype(int filetype, char path[]) {
  bool ok = false;
  
  switch (filetype) {
    case OGG:
      break;
      
    case WAV:
      break;
      
    case FLAC:
      break;
      
#ifdef __AUDIOQUL__
    case MP3:
      break;
      
    case AAC:
      break;
#endif
      
    default:
      break;
  }
  
  return ok;
}

